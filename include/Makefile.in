# for included ICONS
icons.list: $(ICONS) Makefile.am
	( rm -f $@; \
		for image in $(ICONS); do \
			echo $$image | sed s/\.png$$// >> $@; \
			echo $$image >> $@; \
		done )

title.list: $(TITLE) Makefile.am
	( rm -f $@; \
		for image in $(TITLE); do \
			echo $$image | sed s/\.png$$// >> $@; \
			echo $$image >> $@; \
		done )

# the include files
icons.h: $(ICONS) icons.list
	gdk-pixbuf-csource --raw --build-list `cat icons.list` > $@

title.h: $(TITLE) title.list
	gdk-pixbuf-csource --raw --build-list `cat title.list` > $@

@GTK_TRUE@c64_font.h: c64_font.png $(CELLS2C)
@GTK_TRUE@	./cells2c c64_font.png >$@
@GTK_FALSE@c64_font.h: c64_font.png $(CELLS2C)
@GTK_FALSE@	echo; echo; echo "*** GTK+ is needed to regenerate $@"
@GTK_FALSE@	false

@GTK_TRUE@c64_gfx.h: c64_gfx.png $(CELLS2C)
@GTK_TRUE@	./cells2c c64_gfx.png >$@
@GTK_FALSE@c64_gfx.h: c64_gfx.png $(CELLS2C)
@GTK_FALSE@	echo; echo; echo "*** GTK+ is needed to regenerate $@"
@GTK_FALSE@	false

levels.h: $(CAVES) file2c Makefile.am
	sh process.sh $(CAVES)
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
