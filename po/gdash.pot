msgstr ""

#: src/cavedb.c:422
msgid "Foreground color 1 for C64 graphics"
msgstr ""

#: src/cavedb.c:423
msgid "Color 2 (steel wall)"
msgstr ""

#: src/cavedb.c:423
msgid "Foreground color 2 for C64 graphics"
msgstr ""

#: src/cavedb.c:424
msgid "Color 3 (brick wall)"
msgstr ""

#: src/cavedb.c:424
